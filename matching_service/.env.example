# Matching Service - Environment Configuration

PORT=8086
HOST=0.0.0.0

# Database (same as other services)
DATABASE_URL=postgres://postgres:your-password@db.your-project.supabase.co:5432/postgres?sslmode=require

# Auth Service
AUTH_SERVICE_URL=http://localhost:8082

# Gemini AI (required for AI-powered matching)
GEMINI_API_KEY=your-gemini-api-key-here
GEMINI_MODEL=gemini-2.0-flash
EMBEDDING_MODEL=text-embedding-004
GEMINI_TEMPERATURE=0.3

# Cache
CACHE_ENABLED=true
CACHE_TTL=3600

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json
